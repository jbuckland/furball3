extends layout

block content
    each item in data
        div(class='listRow')
            div(class='pilotContainer')
                div(class='pilotCardContainer')
                    img(class='pilotCard' src=item.PilotImgPath)
                div(class='upgradeContainer')
                    each upgradePath in item.UpgradeImgPathList
                        img(class='upgradeCard' src=upgradePath)


